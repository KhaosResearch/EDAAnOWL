@prefix : <https://w3id.org/EDAAnOWL/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix agrovoc: <http://aims.fao.org/aos/agrovoc/> .

@base <https://w3id.org/EDAAnOWL/0.5.0/vocabularies/observed-properties> .

<https://w3id.org/EDAAnOWL/0.5.0/vocabularies/observed-properties> rdf:type owl:Ontology ;
    dct:title "Observed Properties Vocabulary"@en ;
    dct:description """Properties that can be observed or measured in agricultural and environmental datasets.
    
This vocabulary is designed to be extensible. Users can:
1. Use these local concepts directly
2. Reference AGROVOC terms via skos:exactMatch/closeMatch
3. Define new local concepts aligned with external vocabularies

See: http://aims.fao.org/aos/agrovoc/ for the FAO AGROVOC thesaurus."""@en .

:ObservedPropertyScheme rdf:type owl:NamedIndividual ,
                                 skos:ConceptScheme ;
                        dct:description "Properties that can be observed or measured in datasets."@en ;
                        dct:title "Observed Properties"@en .

#################################################################
#    Vegetation Indices (Remote Sensing)
#################################################################

:ndvi rdf:type owl:NamedIndividual ,
               skos:Concept ,
               :ObservableProperty ;
      skos:prefLabel "NDVI"@en ,
                     "NDVI"@es ;
      skos:definition "Normalized Difference Vegetation Index - indicates vegetation health and density."@en ;
      skos:inScheme :ObservedPropertyScheme ;
      skos:exactMatch agrovoc:c_ce585e0d .  # AGROVOC: normalized difference vegetation index

:reflectance_red rdf:type owl:NamedIndividual ,
                          skos:Concept ,
                          :ObservableProperty ;
                 skos:prefLabel "Red Reflectance"@en ,
                                "Reflectancia Roja"@es ;
                 skos:definition "Surface reflectance in the red band (~620-700nm), used in vegetation indices."@en ;
                 skos:inScheme :ObservedPropertyScheme .

:reflectance_nir rdf:type owl:NamedIndividual ,
                          skos:Concept ,
                          :ObservableProperty ;
                 skos:prefLabel "NIR Reflectance"@en ,
                                "Reflectancia NIR"@es ;
                 skos:definition "Surface reflectance in the near-infrared band (~700-1100nm), used in vegetation indices."@en ;
                 skos:inScheme :ObservedPropertyScheme .

:evi rdf:type owl:NamedIndividual ,
              skos:Concept ,
              :ObservableProperty ;
     skos:prefLabel "EVI"@en ;
     skos:definition "Enhanced Vegetation Index - optimized for high biomass regions."@en ;
     skos:inScheme :ObservedPropertyScheme .

:lai rdf:type owl:NamedIndividual ,
              skos:Concept ,
              :ObservableProperty ;
     skos:prefLabel "LAI"@en ,
                    "IAF"@es ;
     skos:definition "Leaf Area Index - total one-sided area of leaf tissue per unit ground area."@en ;
     skos:inScheme :ObservedPropertyScheme ;
     skos:exactMatch agrovoc:c_35196 .  # AGROVOC: leaf area index

:chlorophyllContent rdf:type owl:NamedIndividual ,
                             skos:Concept ,
                             :ObservableProperty ;
                    skos:prefLabel "Chlorophyll Content"@en ,
                                   "Contenido de Clorofila"@es ;
                    skos:definition "Concentration of chlorophyll in plant leaves."@en ;
                    skos:inScheme :ObservedPropertyScheme ;
                    skos:closeMatch agrovoc:c_1577 .  # AGROVOC: chlorophylls

#################################################################
#    Meteorological Properties
#################################################################

:temperature rdf:type owl:NamedIndividual ,
                      skos:Concept ,
                      :ObservableProperty ;
             skos:prefLabel "Temperature"@en ,
                            "Temperatura"@es ;
             skos:definition "Thermal measurement in degrees (air, soil, or water)."@en ;
             skos:inScheme :ObservedPropertyScheme ;
             skos:exactMatch agrovoc:c_7657 .  # AGROVOC: temperature

:precipitation rdf:type owl:NamedIndividual ,
                        skos:Concept ,
                        :ObservableProperty ;
               skos:prefLabel "Precipitation"@en ,
                              "Precipitación"@es ;
               skos:definition "Rainfall, snowfall, or other forms of moisture."@en ;
               skos:inScheme :ObservedPropertyScheme ;
               skos:exactMatch agrovoc:c_6161 .  # AGROVOC: precipitation

:relativeHumidity rdf:type owl:NamedIndividual ,
                           skos:Concept ,
                           :ObservableProperty ;
                  skos:prefLabel "Relative Humidity"@en ,
                                 "Humedad Relativa"@es ;
                  skos:definition "Percentage of water vapor in the air relative to saturation."@en ;
                  skos:inScheme :ObservedPropertyScheme ;
                  skos:exactMatch agrovoc:c_6496 .  # AGROVOC: relative humidity

:windSpeed rdf:type owl:NamedIndividual ,
                    skos:Concept ,
                    :ObservableProperty ;
           skos:prefLabel "Wind Speed"@en ,
                          "Velocidad del Viento"@es ;
           skos:definition "Speed of air movement."@en ;
           skos:inScheme :ObservedPropertyScheme ;
           skos:exactMatch agrovoc:c_37675 .  # AGROVOC: wind velocity

:solarRadiation rdf:type owl:NamedIndividual ,
                         skos:Concept ,
                         :ObservableProperty ;
                skos:prefLabel "Solar Radiation"@en ,
                               "Radiación Solar"@es ;
                skos:definition "Electromagnetic radiation emitted by the sun."@en ;
                skos:inScheme :ObservedPropertyScheme ;
                skos:exactMatch agrovoc:c_14415 .  # AGROVOC: solar radiation

:evapotranspiration rdf:type owl:NamedIndividual ,
                             skos:Concept ,
                             :ObservableProperty ;
                    skos:prefLabel "Evapotranspiration"@en ,
                                   "Evapotranspiración"@es ;
                    skos:definition "Combined water loss from evaporation and plant transpiration."@en ;
                    skos:inScheme :ObservedPropertyScheme ;
                    skos:exactMatch agrovoc:c_2741 .  # AGROVOC: evapotranspiration

#################################################################
#    Soil Properties
#################################################################

:soilMoisture rdf:type owl:NamedIndividual ,
                       skos:Concept ,
                       :ObservableProperty ;
              skos:prefLabel "Soil Moisture"@en ,
                             "Humedad del Suelo"@es ;
              skos:definition "Water content in the soil."@en ;
              skos:inScheme :ObservedPropertyScheme ;
              skos:exactMatch agrovoc:c_7208 .  # AGROVOC: soil moisture

:soilTemperature rdf:type owl:NamedIndividual ,
                          skos:Concept ,
                          :ObservableProperty ;
                 skos:prefLabel "Soil Temperature"@en ,
                                "Temperatura del Suelo"@es ;
                 skos:definition "Temperature of the soil at a given depth."@en ;
                 skos:inScheme :ObservedPropertyScheme ;
                 skos:broader :temperature .

:soilPH rdf:type owl:NamedIndividual ,
                 skos:Concept ,
                 :ObservableProperty ;
        skos:prefLabel "Soil pH"@en ,
                       "pH del Suelo"@es ;
        skos:definition "Measure of acidity or alkalinity of soil."@en ;
        skos:inScheme :ObservedPropertyScheme ;
        skos:exactMatch agrovoc:c_34901 .  # AGROVOC: soil pH

:electricalConductivity rdf:type owl:NamedIndividual ,
                                 skos:Concept ,
                                 :ObservableProperty ;
                        skos:prefLabel "Electrical Conductivity"@en ,
                                       "Conductividad Eléctrica"@es ;
                        skos:altLabel "EC"@en ;
                        skos:definition "Measure of soil salinity via electrical conductivity."@en ;
                        skos:inScheme :ObservedPropertyScheme .

:organicMatter rdf:type owl:NamedIndividual ,
                        skos:Concept ,
                        :ObservableProperty ;
               skos:prefLabel "Organic Matter"@en ,
                              "Materia Orgánica"@es ;
               skos:definition "Percentage of organic material in soil."@en ;
               skos:inScheme :ObservedPropertyScheme ;
               skos:exactMatch agrovoc:c_35657 .  # AGROVOC: soil organic matter

#################################################################
#    Crop Production
#################################################################

:yield rdf:type owl:NamedIndividual ,
                skos:Concept ,
                :ObservableProperty ;
       skos:prefLabel "Crop Yield"@en ,
                      "Rendimiento del Cultivo"@es ;
       skos:definition "Amount of crop produced per unit area."@en ;
       skos:inScheme :ObservedPropertyScheme ;
       skos:exactMatch agrovoc:c_8488 .  # AGROVOC: yield

:yieldForecast rdf:type owl:NamedIndividual ,
                        skos:Concept ,
                        :ObservableProperty ;
               skos:prefLabel "Yield Forecast"@en ,
                              "Predicción de Rendimiento"@es ;
               skos:definition "Predicted crop yield based on models or historical data."@en ;
               skos:inScheme :ObservedPropertyScheme ;
               skos:broader :yield .

:biomass rdf:type owl:NamedIndividual ,
                  skos:Concept ,
                  :ObservableProperty ;
         skos:prefLabel "Biomass"@en ,
                        "Biomasa"@es ;
         skos:definition "Total mass of living organisms or organic matter in an area."@en ;
         skos:inScheme :ObservedPropertyScheme ;
         skos:exactMatch agrovoc:c_926 .  # AGROVOC: biomass

#################################################################
#    Water Management
#################################################################

:waterStress rdf:type owl:NamedIndividual ,
                      skos:Concept ,
                      :ObservableProperty ;
             skos:prefLabel "Water Stress"@en ,
                            "Estrés Hídrico"@es ;
             skos:definition "Condition when water availability limits plant growth."@en ;
             skos:inScheme :ObservedPropertyScheme ;
             skos:exactMatch agrovoc:c_24993 .  # AGROVOC: water stress

:irrigationNeed rdf:type owl:NamedIndividual ,
                         skos:Concept ,
                         :ObservableProperty ;
                skos:prefLabel "Irrigation Need"@en ,
                               "Necesidad de Riego"@es ;
                skos:definition "Estimated water requirement for optimal crop growth."@en ;
                skos:inScheme :ObservedPropertyScheme ;
                skos:broader :waterStress .

#################################################################
#    Pest & Disease
#################################################################

:pestIncidence rdf:type owl:NamedIndividual ,
                        skos:Concept ,
                        :ObservableProperty ;
               skos:prefLabel "Pest Incidence"@en ,
                              "Incidencia de Plagas"@es ;
               skos:definition "Presence or severity of pest infestation."@en ;
               skos:inScheme :ObservedPropertyScheme ;
               skos:exactMatch agrovoc:c_3855 .  # AGROVOC: pest attack

:diseaseIncidence rdf:type owl:NamedIndividual ,
                           skos:Concept ,
                           :ObservableProperty ;
                  skos:prefLabel "Disease Incidence"@en ,
                                 "Incidencia de Enfermedad"@es ;
                  skos:definition "Presence or severity of plant disease."@en ;
                  skos:inScheme :ObservedPropertyScheme ;
                  skos:exactMatch agrovoc:c_5962 .  # AGROVOC: disease incidence

:repiloIncidence rdf:type owl:NamedIndividual ,
                          skos:Concept ,
                          :ObservableProperty ;
                 skos:prefLabel "Repilo Incidence"@en ,
                                "Incidencia de Repilo"@es ;
                 skos:definition "Predicted incidence of Repilo disease (Fusicladium oleagineum) in olive trees."@en ;
                 skos:inScheme :ObservedPropertyScheme ;
                 skos:broader :diseaseIncidence .

#################################################################
#    Generic / Other
#################################################################

:occupancy rdf:type owl:NamedIndividual ,
                    skos:Concept ,
                    :ObservableProperty ;
           skos:prefLabel "Occupancy"@en ,
                          "Ocupación"@es ;
           skos:definition "Usage or occupation rate of facilities."@en ;
           skos:inScheme :ObservedPropertyScheme .
