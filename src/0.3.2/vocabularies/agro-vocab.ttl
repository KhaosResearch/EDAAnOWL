#################################################################

:agro_olive rdf:type owl:NamedIndividual ,
                     skos:Concept ;
            skos:prefLabel "Olive"@en ,
                           "Olivo"@es ;
            skos:definition "Olive tree cultivation and oil production (Olea europaea)."@en ;
            skos:broader :agriculture ;
            skos:inScheme :AgroVocab ;
            skos:exactMatch agrovoc:c_12926 .  # AGROVOC: olives

:agro_vine rdf:type owl:NamedIndividual ,
