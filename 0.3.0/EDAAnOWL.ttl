#################################################################

###  http://purl.org/dc/terms/abstract
dcterms:abstract rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/bibliographicCitation
dcterms:bibliographicCitation rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/action
odrl:action rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/constraint
odrl:constraint rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/leftOperand
odrl:leftOperand rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/operator
odrl:operator rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/permission
odrl:permission rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/rightOperand
odrl:rightOperand rdf:type owl:AnnotationProperty .


###  https://schema.org/codeRepository
schema:codeRepository rdf:type owl:AnnotationProperty .


###  https://schema.org/image
schema:image rdf:type owl:AnnotationProperty .


###  https://w3id.org/widoco/vocab#introduction
widoco:introduction rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#duration
xsd:duration rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/conformsTo
dcterms:conformsTo rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/source
dcterms:source rdf:type owl:ObjectProperty ;
               rdfs:comment "A related resource from which the described resource is derived (e.g., the originating catalog)."@en ;
               rdfs:label "source"@en .


###  http://purl.org/dc/terms/subject
dcterms:subject rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/dcat#contactPoint
dcat:contactPoint rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/dcat#theme
dcat:theme rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasDerivedFrom
prov:wasDerivedFrom rdf:type owl:ObjectProperty ;
                    rdfs:domain :DataAsset ;
                    rdfs:range :DataAsset ;
                    rdfs:comment "A derivation is a transformation of an entity into another, an update of an entity resulting in a new one, or the construction of a new entity based on a pre-existing entity. In EDAAnOWL, this links a derived DataAsset to its source DataAsset."@en ;
                    rdfs:label "was derived from"@en .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:domain :DataAsset ;
                    rdfs:range ids:DataApp ;
                    rdfs:comment "Generation is the completion of production of a new entity by an activity. This entity did not exist before generation and becomes available for usage after this generation. In EDAAnOWL, this links a DataAsset to the DataApp that generated it."@en ;
                    rdfs:label "was generated by"@en .


###  https://w3id.org/EDAAnOWL/conformsToProfile
:conformsToProfile rdf:type owl:ObjectProperty ;
                   rdfs:domain ids:Resource ;
                   rdfs:range :DataProfile ;
                   rdfs:comment "Links a resource (e.g., a DataAsset) to a DataProfile that describes its structure and characteristics."@en ;
                   rdfs:label "conforms to profile"@en .


###  https://w3id.org/EDAAnOWL/consumesResource
:consumesResource rdf:type owl:ObjectProperty ;
                  rdfs:domain bigwf:Component ;
                  rdfs:range ids:Representation ;
                  rdfs:comment "Links a BIGOWL Component to an IDSA Representation it consumes as input."@en ;
                  rdfs:label "consumes representation"@en .


###  https://w3id.org/EDAAnOWL/declaresDataClass
:declaresDataClass rdf:type owl:ObjectProperty ;
                   rdfs:domain :DataProfile ;
                   rdfs:range bigdat:Data ;
                   rdfs:comment "Links a DataProfile to a BIGOWL Data class (e.g., bigdat:TabularDataSet, bigdat:Image)."@en ;
                   rdfs:label "declares data class"@en .


###  https://w3id.org/EDAAnOWL/declaresObservedProperty
:declaresObservedProperty rdf:type owl:ObjectProperty ;
                          rdfs:domain :DataProfile ;
                          rdfs:range skos:Concept ;
                          rdfs:comment "Links a DataProfile to a semantic concept it describes, typically from a controlled vocabulary (e.g., an AGROVOC term)."@en ;
                          rdfs:label "declares observed property"@en .


###  https://w3id.org/EDAAnOWL/externalQualityReport
:externalQualityReport rdf:type owl:ObjectProperty ;
                       rdfs:domain ids:Resource ;
                       rdfs:range :QualityReport ;
                       rdfs:comment "Links a resource to a structured object describing an external quality report."@en ;
                       rdfs:label "external quality report"@en .


###  https://w3id.org/EDAAnOWL/hasCRS
:hasCRS rdf:type owl:ObjectProperty ;
        rdfs:domain :DataProfile ;
        rdfs:range ids:Resource ;
        rdfs:comment "Links a Data Profile to the formal definition of its Coordinate Reference System (CRS), preferably using an EPSG URI (e.g., http://www.opengis.net/def/crs/EPSG/0/4326)."@en ;
        rdfs:label "has Coordinate Reference System"@en .


###  https://w3id.org/EDAAnOWL/hasDomainSector
:hasDomainSector rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dcat:theme ;
                 rdfs:domain ids:DataApp ,
                             ids:Resource ;
                 rdfs:range skos:Concept ;
                 rdfs:comment "A sub-property of dcat:theme, specifically linking a resource to a concept from a domain sector vocabulary."@en ;
                 rdfs:label "has domain sector"@en .


###  https://w3id.org/EDAAnOWL/hasMetric
:hasMetric rdf:type owl:ObjectProperty ;
           rdfs:domain :DataProfile ;
           rdfs:range :Metric ;
           rdfs:comment "Links a Data Profile to a specific quality, performance, or descriptive metric."@en ;
           rdfs:label "has metric"@en .


###  https://w3id.org/EDAAnOWL/hasSpatialCoverage
:hasSpatialCoverage rdf:type owl:ObjectProperty ;
                    rdfs:domain :SpatialTemporalAsset ;
                    rdfs:range locn:Geometry ;
                    rdfs:comment "Defines the spatial coverage of the data asset required by Earth Observation CUs."@en ;
                    rdfs:label "has spatial coverage"@en .


###  https://w3id.org/EDAAnOWL/hasTemporalCoverage
:hasTemporalCoverage rdf:type owl:ObjectProperty ;
                     rdfs:domain :SpatialTemporalAsset ;
                     rdfs:range time:Interval ;
                     rdfs:comment "Defines the temporal coverage of the data asset required by Earth Observation CUs."@en ;
                     rdfs:label "has temporal coverage"@en .


###  https://w3id.org/EDAAnOWL/implementsComponent
:implementsComponent rdf:type owl:ObjectProperty ;
                     rdfs:domain ids:DataApp ;
                     rdfs:range bigwf:Component ;
                     rdfs:comment "Links an IDSA DataApp to the BIGOWL workflow Component it implements."@en ;
                     rdfs:label "implements component"@en .


###  https://w3id.org/EDAAnOWL/legalContact
:legalContact rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf dcat:contactPoint ;
              rdfs:domain ids:Resource ;
              rdfs:range vcard:Kind ;
              rdfs:comment "A specific contact point for legal inquiries about the resource."@en ;
              rdfs:label "legal contact"@en .


###  https://w3id.org/EDAAnOWL/parameter
:parameter rdf:type owl:ObjectProperty ;
           rdfs:domain ids:DataApp ;
           rdfs:range <http://www.e-lico.eu/ontologies/dmo/DMOP/DMOP.owl#Parameter> ;
           rdfs:comment "Links a DataApp to an execution parameter it accepts."@en ;
           rdfs:label "parameter"@en .


###  https://w3id.org/EDAAnOWL/producesObservableProperty
:producesObservableProperty rdf:type owl:ObjectProperty ;
                            rdfs:domain ids:SmartDataApp ;
                            rdfs:range :ObservableProperty ;
                            rdfs:comment "Defines the semantic signature of the OUTPUT of a service (e.g., produces Crop_Forecast)."@en ;
                            rdfs:label "produces observable property"@en .


###  https://w3id.org/EDAAnOWL/producesProfile
:producesProfile rdf:type owl:ObjectProperty ;
                 rdfs:domain ids:DataApp ;
                 rdfs:range :DataProfile ;
                 rdfs:comment "Links a DataApp to a DataProfile it produces as output."@en ;
                 rdfs:label "produces profile"@en .


###  https://w3id.org/EDAAnOWL/producesResource
:producesResource rdf:type owl:ObjectProperty ;
                  rdfs:domain bigwf:Component ;
                  rdfs:range ids:Representation ;
                  rdfs:comment "Links a BIGOWL Component to an IDSA Representation it produces as output."@en ;
                  rdfs:label "produces representation"@en .


###  https://w3id.org/EDAAnOWL/realizesWorkflow
:realizesWorkflow rdf:type owl:ObjectProperty ;
                  rdfs:domain ids:DataApp ;
                  rdfs:range opmw:WorkflowTemplate ;
                  rdfs:comment "Links a DataApp to the OPMW Workflow Template it executes."@en ;
                  rdfs:label "realizes workflow"@en .


###  https://w3id.org/EDAAnOWL/requiresObservableProperty
:requiresObservableProperty rdf:type owl:ObjectProperty ;
                            rdfs:domain ids:SmartDataApp ;
                            rdfs:range :ObservableProperty ;
                            rdfs:comment "Defines the semantic signature of INPUT for a service (e.g., requires NDVI)."@en ;
                            rdfs:label "requires observable property"@en .


###  https://w3id.org/EDAAnOWL/requiresProfile
:requiresProfile rdf:type owl:ObjectProperty ;
                 rdfs:domain ids:DataApp ;
                 rdfs:range :DataProfile ;
                 rdfs:comment "Links a DataApp to a DataProfile it requires as input."@en ;
                 rdfs:label "requires profile"@en .


###  https://w3id.org/EDAAnOWL/servesObservableProperty
:servesObservableProperty rdf:type owl:ObjectProperty ;
                          rdfs:domain :DataAsset ;
                          rdfs:range :ObservableProperty ;
                          rdfs:comment "Links a data asset (DataAsset) to the semantic variable it describes (e.g., NDVI)."@en ;
                          rdfs:label "serves observable property"@en .


###  https://w3id.org/EDAAnOWL/spatialGranularityConcept
:spatialGranularityConcept rdf:type owl:ObjectProperty ;
                           rdfs:domain ids:Resource ;
                           rdfs:range skos:Concept ;
                           rdfs:comment "Links a resource to a SKOS concept describing its spatial granularity (e.g., 'Municipal', 'Provincial')."@en ;
                           rdfs:label "spatial granularity (concept)"@en .


###  https://w3id.org/EDAAnOWL/supportContact
:supportContact rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dcat:contactPoint ;
                rdfs:domain ids:Resource ;
                rdfs:range vcard:Kind ;
                rdfs:comment "Standard contact point for support, using vCard (fn, email, telephone, URL)."@en ;
                rdfs:label "support contact"@en .


###  https://w3id.org/EDAAnOWL/topic
:topic rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf dcterms:subject ;
       rdfs:domain ids:Resource ;
       rdfs:range skos:Concept ;
       rdfs:comment "A sub-property of dct:subject for linking a resource to a descriptive SKOS concept (e.g., from AGROVOC)."@en ;
       rdfs:label "topic"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/alternative
dcterms:alternative rdf:type owl:DatatypeProperty ;
                    rdfs:comment "An alternative name for the resource."@en ;
                    rdfs:label "alternative name"@en .


###  http://www.w3.org/ns/dcat#spatialResolutionInDegrees
dcat:spatialResolutionInDegrees rdf:type owl:DatatypeProperty ;
                                rdfs:range xsd:decimal ;
                                rdfs:comment "The spatial resolution of the dataset, expressed in decimal degrees."@en .


###  http://www.w3.org/ns/dcat#spatialResolutionInMeters
dcat:spatialResolutionInMeters rdf:type owl:DatatypeProperty ;
                               rdfs:range xsd:decimal ;
                               rdfs:comment "The spatial resolution of the dataset, expressed in meters."@en .


###  http://www.w3.org/ns/dcat#temporalResolution
dcat:temporalResolution rdf:type owl:DatatypeProperty ;
                        rdfs:range xsd:duration ;
                        rdfs:comment "The temporal resolution of the dataset, expressed as an xsd:duration (e.g., 'P1D' for daily)."@en .


###  https://schema.org/thumbnailUrl
schema:thumbnailUrl rdf:type owl:DatatypeProperty ;
                    rdfs:domain ids:Resource ;
                    rdfs:range xsd:anyURI ;
                    rdfs:comment "A URL pointing to a thumbnail image for the resource."@en ;
                    rdfs:label "thumbnail URL"@en .


###  https://w3id.org/EDAAnOWL/accessType
:accessType rdf:type owl:DatatypeProperty ;
            rdfs:domain ids:Resource ;
            rdfs:range xsd:string ;
            rdfs:comment "Indicates the primary access mode for the resource (e.g., download, compute, ...)."@en ;
            rdfs:label "access type"@en .


###  https://w3id.org/EDAAnOWL/alternativeName
:alternativeName rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf dcterms:alternative ;
                 rdfs:domain ids:Resource ;
                 rdfs:range rdfs:Literal ;
                 rdfs:comment "Alternative title or name for a resource."@en ;
                 rdfs:label "alternate name"@en .


###  https://w3id.org/EDAAnOWL/appliesToFeature
:appliesToFeature rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Metric ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indicates the specific feature, column, or attribute to which a metric refers. Used when a metric is not global to the dataset (e.g., 'column_name', 'user.address.city', '$.user.address.city')."@en ;
                  rdfs:label "applies to feature"@en .


###  https://w3id.org/EDAAnOWL/auditLogAvailable
:auditLogAvailable rdf:type owl:DatatypeProperty ;
                   rdfs:domain ids:Resource ;
                   rdfs:range xsd:boolean ;
                   rdfs:comment "Indicates if audit logs for the resource usage are available."@en ;
                   rdfs:label "audit log available"@en .


###  https://w3id.org/EDAAnOWL/computedAt
:computedAt rdf:type owl:DatatypeProperty ;
            rdfs:domain :Metric ;
            rdfs:range xsd:dateTime ;
            rdfs:comment "The timestamp indicating when the metric's value was computed or validated."@en ;
            rdfs:label "computed at"@en .


###  https://w3id.org/EDAAnOWL/isAlive
:isAlive rdf:type owl:DatatypeProperty ;
         rdfs:domain ids:Resource ;
         rdfs:range xsd:boolean ;
         rdfs:comment "Indicates if the dataset is actively maintained and expects future updates."@en ;
         rdfs:label "is alive"@en .


###  https://w3id.org/EDAAnOWL/knownLimitations
:knownLimitations rdf:type owl:DatatypeProperty ;
                  rdfs:domain ids:Resource ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Human-readable description of any known limitations, biases, or quality issues of the resource."@en ;
                  rdfs:label "known limitations"@en .


###  https://w3id.org/EDAAnOWL/metricName
:metricName rdf:type owl:DatatypeProperty ;
            rdfs:domain :Metric ;
            rdfs:range xsd:string ;
            rdfs:comment "A human-readable name or identifier for the metric (e.g., 'recordCount', 'missing_ratio')."@en ;
            rdfs:label "metric name"@en .


###  https://w3id.org/EDAAnOWL/metricUnit
:metricUnit rdf:type owl:DatatypeProperty ;
            rdfs:domain :Metric ;
            rdfs:range xsd:string ;
            rdfs:comment "The unit of measurement for the metric value (e.g., '%', 'kg/ha', 'ppm')."@en ;
            rdfs:label "metric unit"@en .


###  https://w3id.org/EDAAnOWL/metricValue
:metricValue rdf:type owl:DatatypeProperty ;
             rdfs:domain :Metric ;
             rdfs:range rdfs:Literal ;
             rdfs:comment "The value of the metric. The range is rdfs:Literal to support multiple datatypes (e.g., xsd:boolean, xsd:integer, xsd:string, xsd:decimal)."@en ;
             rdfs:label "metric value"@en .


###  https://w3id.org/EDAAnOWL/paymentModelDescription
:paymentModelDescription rdf:type owl:DatatypeProperty ;
                         rdfs:domain ids:Resource ;
                         rdfs:range xsd:string ;
                         rdfs:comment "A human-readable description of the pricing or payment model (e.g., 'Monthly Subscription', 'Pay-per-use'). This complements the formal ODRL policy."@en ;
                         rdfs:label "payment model description"@en .


###  https://w3id.org/EDAAnOWL/profileCRS
:profileCRS rdf:type owl:DatatypeProperty ;
            rdfs:domain :DataProfile ;
