  "@type" : [ "http://www.w3.org/2002/07/owl#Ontology" ],
  "http://purl.org/dc/terms/abstract" : [ {
    "@language" : "en",
    "@value" : "Lightweight ontology for data spaces. It aligns IDSA and BIGOWL, focusing on the strong semantic description of Data Assets and Data Apps through Data Profiles, which include technical characteristics (DCAT 3) and granular quality metrics."
  } ],
  "http://purl.org/dc/terms/bibliographicCitation" : [ {
    "@value" : "Salvachúa Galimberti, M. J.; Khaos Research. (2025). EDAAnOWL: An ontology for annotating data-space assets aligned with IDSA and BIGOWL. Version 0.3.0."
  } ],
  "http://purl.org/dc/terms/created" : [ {
    "@type" : "http://www.w3.org/2001/XMLSchema#date",
    "@value" : "2025-10-10"
  } ],
  "http://purl.org/dc/terms/creator" : [ {
    "@id" : "https://github.com/MartinM10"
  }, {
    "@id" : "https://khaos.uma.es/"
  }, {
    "@id" : "https://www.linkedin.com/in/martin-jeronimo-salvachua/"
  } ],
  "http://purl.org/dc/terms/description" : [ {
    "@language" : "en",
    "@value" : "Ontology for data spaces that bridges the IDSA Information Model with the BIGOWL analytical family. Its core function is to describe `DataAssets` (assets) and `DataApps` (services) using `DataProfiles` (profiles): semantic models that detail technical specifications (like DCAT 3 resolutions, CRS) and granular quality metrics (like completeness or uniqueness per feature). This enables robust, automated discovery and compatibility checking (matchmaking) for data and services."
  } ],
  "http://purl.org/dc/terms/license" : [ {
    "@id" : "https://creativecommons.org/licenses/by/4.0/"
  } ],
  "http://purl.org/dc/terms/modified" : [ {
    "@type" : "http://www.w3.org/2001/XMLSchema#date",
    "@value" : "2025-11-19"
  } ],
  "http://purl.org/dc/terms/title" : [ {
    "@language" : "en",
    "@value" : "EDAAnOWL Ontology"
  } ],
