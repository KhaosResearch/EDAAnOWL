                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A class representing a single, measurable characteristic of an asset or profile. Aligned with dqv:Metric.</rdfs:comment>
