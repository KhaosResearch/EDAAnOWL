                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :metricValue ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ;
        rdfs:comment "A class representing a single, measurable characteristic of an asset or profile. Aligned with dqv:Metric. Can optionally use :metricType for standardized metric names."@en ,
                     "Una clase que representa una característica única y medible de un activo o perfil. Alineada con dqv:Metric. Opcionalmente puede usar :metricType para nombres de métricas estandarizados."@es ;
        rdfs:label "Metric"@en ,
                   "Métrica"@es .


